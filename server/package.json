{
  "name": "fristrack-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --coverage --verbose",
    "check": "biome check ./routes",
    "check:fix": "biome check --write ./routes",
    "format": "biome format ./routes",
    "format:fix": "biome format --write ./routes",
    "lint": "biome lint ./routes",
    "lint:fix": "biome lint --write ./routes",
    "ci": "biome ci ./routes"
  },
  "dependencies": {
    "argon2": "^0.44.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "jest": "^30.2.0",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.1.2",
    "react-native-worklets": "0.5.1",
    "supertest": "^7.1.4",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "nodemon": "^3.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
}
